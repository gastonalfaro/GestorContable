      DTS:CreationName=""
      DTS:DTSID="{240A8845-5D1F-482B-8F96-93336BEF0FC2}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT IDEntidad, UnidadTiempoPeriodo, Periodo,&#xA;IdEstadoFinanciero,&#xA;0 AS NumConsecutivo, CodigoSegmento,&#xA;NumCuenta,&#xA;CuentaConcepto,&#xA;SaldoInicial,&#xA;DebitosPeriodo,&#xA;CreditosPeriodo,&#xA;SaldoFinal,&#xA;UsuarioCarga, &#xA;FechaDeCarga&#xA;FROM&#xA;(SELECT 1 AS IdJoin,&#xA;IDEntidad, UnidadTiempoPeriodo, Periodo&#xA;FROM [Data$B1:D2]) As Data&#xA;INNER JOIN &#xA;(SELECT &#xA;1 AS IdJoin,&#xA;&quot; + @[User::IdEstadoFinanciero] + &quot; AS IdEstadoFinanciero,&#xA;[CODIGO SEGMENTO] AS CodigoSegmento,&#xA;[CUENTA (REPORTAR MÁXIMO A NIVEL 8)] AS NumCuenta,&#xA;[NOMBRE CUENTA] AS CuentaConcepto,&#xA;[SALDO INICIAL] AS SaldoInicial,&#xA;[DEBITOS PERIODO] AS DebitosPeriodo,&#xA;[CREDITOS PERIODO] AS CreditosPeriodo,&#xA;[SALDO FINAL] AS SaldoFinal,&#xA;'&quot; + @[User::UsuarioCarga] +&quot;' AS UsuarioCarga, &#xA;NOW() AS FechaDeCarga&#xA;FROM [BalanceComprobacion$A9:P]&#xA;WHERE [SALDO INICIAL] IS NOT NULL) AS BalanceComprobacion&#xA;ON Data.IdJoin = BalanceComprobacion.IdJoin&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_Carga">
      <DTS:VariableValue
        DTS:DataType="8">SELECT IDEntidad, UnidadTiempoPeriodo, Periodo,
IdEstadoFinanciero,
0 AS NumConsecutivo, CodigoSegmento,
NumCuenta,
CuentaConcepto,
SaldoInicial,
DebitosPeriodo,
CreditosPeriodo,
SaldoFinal,
UsuarioCarga, 
FechaDeCarga
FROM
(SELECT 1 AS IdJoin,
IDEntidad, UnidadTiempoPeriodo, Periodo
FROM [Data$B1:D2]) As Data
INNER JOIN 
(SELECT 
1 AS IdJoin,
5 AS IdEstadoFinanciero,
[CODIGO SEGMENTO] AS CodigoSegmento,
[CUENTA (REPORTAR MÁXIMO A NIVEL 8)] AS NumCuenta,
[NOMBRE CUENTA] AS CuentaConcepto,
[SALDO INICIAL] AS SaldoInicial,
[DEBITOS PERIODO] AS DebitosPeriodo,
[CREDITOS PERIODO] AS CreditosPeriodo,
[SALDO FINAL] AS SaldoFinal,
'SistemaCargaDTSX' AS UsuarioCarga, 
NOW() AS FechaDeCarga
FROM [BalanceComprobacion$A9:P]
WHERE [SALDO INICIAL] IS NOT NULL) AS BalanceComprobacion
ON Data.IdJoin = BalanceComprobacion.IdJoin</DTS:VariableValue>
    </DTS:Variable>
