<%@ Page Title="" Language="C#" MasterPageFile="~/PaginaMaestra/PortalPrincipal.Master" AutoEventWireup="true" CodeBehind="frmNuevoParametro.aspx.cs" Inherits="Presentacion.Mantenimiento.Gestiones.frmNuevoParametro" %>
<asp:Content ID="Content1" ContentPlaceHolderID="ContenidoJS" runat="server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ScriptContent" runat="server">
</asp:Content>
<asp:Content ID="Content3" ContentPlaceHolderID="encabezado" runat="server">
</asp:Content>
<asp:Content ID="Content4" ContentPlaceHolderID="Enlaces" runat="server">
</asp:Content>
<asp:Content ID="Content5" ContentPlaceHolderID="Contenido" runat="server">
    <div class="FormatoBotones"><asp:Button ID="btnParametroVolver" runat="server" Text="VOLVER" OnClick="btnParametroVolver_Click" CssClass="ButtonNeutro"/></div> 
    <div class="col-md-12">
        <h3>Parámetros</h3>
        <p>Mantenimiento de Parámetros del Sistema Gestor.</p>
        <div class="col-md-6">
            <div class="col-md-3"><asp:Label ID="Label1" runat="server" Text="Código:" Font-Bold="true"></asp:Label>   </div>
            <div class="col-md-8"><asp:TextBox ID="txtIdParametro" runat="server" CssClass="FormatoTextBox"></asp:TextBox></div>
        </div>
        <div class="col-md-6">
            <div class="col-md-3"> <asp:Label ID="Label3" runat="server" Text="Fecha Vigencia:" Font-Bold="true"></asp:Label>  </div>
            <div class="col-md-8">
                 <asp:TextBox  ID="txtFchVigencia" runat="server" CssClass="js-date-picker FormatoTextBox"> </asp:TextBox>
            </div>
        </div>
        <div class="col-md-6">
            <div class="col-md-3"><asp:Label ID="Label4" runat="server" Text="Módulo:" Font-Bold="true"></asp:Label> </div>
            <div class="col-md-8"> <asp:DropDownList ID="ddlModulo" runat="server" CssClass="FormatoDropDownList" ></asp:DropDownList></div>
        </div>
        <div class="col-md-6">
            <div class="col-md-3"> <asp:Label ID="Label2" runat="server" Text="Descripción:" Font-Bold="true"></asp:Label></div>
            <div class="col-md-8"><asp:TextBox  ID="txtDesParametro" runat="server" CssClass="FormatoTextBox"></asp:TextBox></div>
        </div>
        <div class="col-md-6">
            <div class="col-md-3"><asp:Label ID="Label5" runat="server" Text="Tipo:" Font-Bold="true"></asp:Label></div>
            <div class="col-md-8"><asp:TextBox  ID="txtTipoParametro" runat="server" CssClass="FormatoTextBox"></asp:TextBox></div>
        </div>
        <div class="col-md-6">
            <div class="col-md-3"><asp:Label ID="Label6" runat="server" Text="Valor:" Font-Bold="true"></asp:Label></div>
            <div class="col-md-8"><asp:TextBox Width="100%"  ID="txtValor" runat="server" CssClass="FormatoTextBox"></asp:TextBox></div>
        </div>
        <div class="col-md-12" style="text-align:center;"><asp:Button ID="btnCrearParametro" runat="server" Text="CREAR" OnClick="btnCrearParametro_Click" CssClass="ButtonNeutro"/></div>
    </div>
    <div class="col-md-12" ><asp:label ID="lblMensaje" runat="server" Visible="false" Font-Bold="true" ></asp:label></div>

</asp:Content>
