      <Query>
        <DataSourceName>DSGestNICSPDEV</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        co.Resoluciones.IdResolucion, co.Resoluciones.IdExpedienteFK, co.CobrosPagos.MontoPrincipal, co.CobrosPagos.MontoIntereses, co.CobrosPagos.MontoPrincipalColones, 
                         co.CobrosPagos.MontoInteresesColones, co.Resoluciones.EstadoResolucion, co.Expedientes.IdSociedadGL, ma.SociedadesGL.NomSociedad
FROM            co.Resoluciones INNER JOIN
                         co.Expedientes ON co.Resoluciones.IdExpedienteFK = co.Expedientes.IdExpediente INNER JOIN
                         co.CobrosPagos ON co.Resoluciones.IdResolucion = co.CobrosPagos.IdResolucionFK INNER JOIN
                         ma.SociedadesGL ON co.Expedientes.IdSociedadGL = ma.SociedadesGL.IdSociedadGL
WHERE        (co.Resoluciones.EstadoResolucion = ?) AND (co.Expedientes.FchCreacion BETWEEN ? AND ?) AND (co.Expedientes.EstadoExpediente = 'Activo') AND (co.Expedientes.IdSociedadGL IN (?))</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
