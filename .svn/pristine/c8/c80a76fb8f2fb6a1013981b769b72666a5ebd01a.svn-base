using Datos.ConexionSQL;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace LogicaNegocio.Contingentes
{
    public class clsEntidadPersonas : clsProcedimientoAlmacenado
    {

        #region Variables
        string lstr_IdExpediente = string.Empty;//Llave foránea que liga a la persona demandada/demandante con el expediente
        string lstr_CedDemandado = string.Empty;//Cédula del demandado
        string lstr_NombreDemandado = string.Empty;//Nombre del demandado
        string lstr_CedActor = string.Empty;//Cédula del actor
        string lstr_NombreActor = string.Empty;//Nombre del actor
        string lstr_TipoEntidadPersona = string.Empty;//Identificador que distingue si es un actor o un demandante
        string lstr_Porcentaje = string.Empty;//Nombre del Centro de Beneficio

        #endregion

        #region Asignaciones/Obtencionres
        public string Lstr_IdExpediente
        {
            get { return lstr_IdExpediente; }
            set { lstr_IdExpediente = value; }
