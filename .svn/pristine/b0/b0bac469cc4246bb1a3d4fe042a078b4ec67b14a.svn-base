using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace LogicaNegocio.Consolidacion
{
    public class tBalanceComprobacionPosicion
    {
        #region variables
        private string lstr_posicion;
        private string lstr_subposicion;
        private string lstr_unid_asocia;
        private string lstr_moneda;
        private string lstr_signo;
        private decimal ldec_valor_ml;
        private decimal ldec_valor_mt;
        private decimal ldec_valor_ac;

        #endregion

        #region obtencion y asignacion

        public string Lstr_posicion
        {
            get { return lstr_posicion; }
            set { lstr_posicion = value; }
        }

        public string Lstr_subposicion
        {
            get { return lstr_subposicion; }
            set { lstr_subposicion = value; }
        }
        public string Lstr_unid_asocia
        {
            get { return lstr_unid_asocia; }
            set { lstr_unid_asocia = value; }
        }

        public string Lstr_moneda
        {
            get { return lstr_moneda; }
            set { lstr_moneda = value; }
        }

        public string Lstr_signo
        {
            get { return lstr_signo; }
            set { lstr_signo = value; }
        }

        public decimal Ldec_valor_ml
        {
            get { return ldec_valor_ml; }
            set { ldec_valor_ml = value; }
        }

        public decimal Ldec_valor_mt
        {
            get { return ldec_valor_mt; }
            set { ldec_valor_mt = value; }
        }

        public decimal Ldec_valor_ac
        {
            get { return ldec_valor_ac; }
            set { ldec_valor_ac = value; }
        }

        #endregion

        #region procedimientos
        private tBalanceComprobacionPosicion() { }

        public tBalanceComprobacionPosicion(string str_posicion, string str_subposicion, string str_unid_asocia,
            string str_moneda, string str_signo, decimal dec_valor_ml, decimal dec_valor_mt, decimal dec_valor_ac)
        {
            lstr_posicion = str_posicion;
            lstr_subposicion = str_subposicion;
            lstr_unid_asocia = str_unid_asocia;
            lstr_moneda = str_moneda;
            lstr_signo = str_signo;
            ldec_valor_ml = dec_valor_ml;
            ldec_valor_mt = dec_valor_mt;
            ldec_valor_ac = dec_valor_ac; 
        }
        #endregion

    }
}