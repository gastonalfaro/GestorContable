* {
    margin:0;
    padding:0;
}

html, body, form {
    height:100%;
}


.wrapper {
    min-height: 100%;
    position: relative;
}

.encabezado {
    /*height: 100%;*/
    display: block;
    margin-left: auto;
    margin-right: auto;
    background: linear-gradient(#0175BE, #233072);
    max-width: 95%;
}

.separador {
    height: 30px;
    background: #ffffff;
    margin-left: auto;
    margin-right: auto;
    max-width: 95%;
}

.contenidoseparador {
    padding-left: 40px;
    padding-right: 40px;
    padding-bottom: 4px;
    padding-top: 4px;
    font: 12px arial;
    text-align: end;
}

.push {
    /*clear: both;
    height: 124px;*/
}
.piepagina{
    clear: both;
    height: 124px;
    background: linear-gradient(#0175BE, #233072);
    position: relative;
    width: 100%;
    bottom: 0;
    left: 0;
}

.contacto {
    margin-left: 40px;
    display: block;
    width: 50%;
    float: left;
    visibility: visible;
    display: inline;
    font: 10px arial;
    color: #ffffff;
    letter-spacing: 0.8px;
    line-height: 14px;
    padding: 10px;
}

.contenidopiepagina {
    display: block;
    padding-right: 40px;
    height: inherit;
    width: inherit;
}

.imagenencabezado {
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: 10px;
    margin-bottom: 0px;
    height: auto;
    width: 95%;
}

.imagenpiepagina {
    display: block;
    margin-left: auto;
    padding-top: 35px;
    height: 85%;
}

.imagendinamica {
    display: block;
    margin-left: auto;
    margin-right: auto;
}

.fondoprincipal {
    background-image: url(../imagenes/fondo.png);
    background-repeat: repeat;
    margin: auto;
}

.fondosecundario {
    background: #ffffff;
    font: 12px arial;
    width: 95%;
    display: block;
    /*margin-left: 33px;
    margin-right: auto;*/
    margin: 0 auto;
    padding-top: 10px;
    padding-bottom: 10px;
    min-height:100%;
    position:relative;

    
}

.contenido {
    width: 70%;
    /*float: right;*/
    margin-top: 20px;
    margin-bottom: 124px;
    margin-left: auto;
    margin-right: auto;
    padding-bottom: 124px;
}

