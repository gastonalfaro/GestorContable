          <QueryParameter Name="@FechaHasta">
            <Value>=Parameters!FechaHasta.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        IdAuxCont, FchAccion, Nemotecnico, Plazo, Propietario, CuentaAfectada, DescripcionCuenta, TpoCambioUDE, MontoUDE, IdClaveContable, DesTipoOperacion, 
                         UsrCreacion, FchCreacion, UsrModifica, FchModifica
FROM            cf.AuxiliarUdes
WHERE        (Nemotecnico = @Nemotecnico OR
                         ISNULL(@Nemotecnico, '') = '') AND (Plazo = @Plazo OR
                         ISNULL(@Plazo, '') = '') AND (Propietario = @Propietario OR
                         ISNULL(@Propietario, '') = '') AND (CuentaAfectada = @CuentaAfectada OR
                         ISNULL(@CuentaAfectada, '') = '') AND (FchAccion &gt;= @FechaDesde) AND (FchAccion &lt;= @FechaHasta)</CommandText>
      </Query>
