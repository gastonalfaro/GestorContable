using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace LogicaNegocio.Contingentes
{
    public class clsArchivos
    {
        #region parametros 
        private int lint_IdArchivo;
        private string lstr_IdResolucionFK;
        private string lstr_NombreArchivo;
        private string lstr_UsrCreacion;
        private string lstr_UsrModifica;
        #endregion

        #region constructores
        /// <summary>
        /// 
        /// </summary>
        public clsArchivos(){}
        /// <summary>
        /// 
        /// </summary>
        /// <param name="lint_IdArchivo"></param>
        /// <param name="lstr_IdResolucionFK"></param>
        /// <param name="lstr_NombreArchivo"></param>
        /// <param name="lstr_UsrCreacion"></param>
        /// <param name="lstr_UsrModifica"></param>
        public clsArchivos(int lint_IdArchivo,string lstr_IdResolucionFK,string lstr_NombreArchivo,string lstr_UsrCreacion,string lstr_UsrModifica) {
            
            this.lint_IdArchivo = lint_IdArchivo;
            this.lstr_IdResolucionFK = lstr_IdResolucionFK;//Id de resolucion asociada al archivo
            this.lstr_NombreArchivo = lstr_NombreArchivo;//Nombre del archivo
            this.lstr_UsrCreacion = lstr_UsrCreacion;//Bitacora de creacion 
            this.lstr_UsrModifica = lstr_UsrModifica;//Bitacora de modificacion
        
        }
        #endregion 

        #region obtención y asignación
        /// <summary>
        /// 
        /// </summary>
        public int Lint_IdArchivo
        {
            get { return lint_IdArchivo; }
            set { lint_IdArchivo = value; }
        }
        /// <summary>
        /// 
        /// </summary>
        public string Lstr_IdResolucionFK
        {
            get { return lstr_IdResolucionFK; }
            set { lstr_IdResolucionFK = value; }
        }
        /// <summary>
        /// 
        /// </summary>
        public string Lstr_NombreArchivo
        {
            get { return lstr_NombreArchivo; }
            set { lstr_NombreArchivo = value; }
        }
        /// <summary>
        /// 
        /// </summary>
        public string Lstr_UsrCreacion
        {
            get { return lstr_UsrCreacion; }
            set { lstr_UsrCreacion = value; }
        }
        /// <summary>
        /// 
        /// </summary>
        public string Lstr_UsrModifica
        {
            get { return lstr_UsrModifica; }
            set { lstr_UsrModifica = value; }
        }
        #endregion
    }
}